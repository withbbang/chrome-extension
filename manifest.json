{
  "name": "단어 사전 & 구문 해석",
  "description": "단어 사전 & 구문 해석",
  "version": "1.0",
  "manifest_version": 2,
  "permissions": ["<all_urls>", "storage"],
  "background": {
    "scripts": ["background.js", "env.js"]
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content_script.js"],
      "css": ["content_script.css"]
    }
  ],
  "browser_action": {
    "default_title": "단어 사전 & 구문 해석",
    "default_popup": "popup.html",
    "default_icon": {
      "48": "icons/earth.png"
    }
  },
  "icons": {
    "48": "icons/earth.png"
  },
  "commands": {
    "toggle_command": {
      "suggested_key": {
        "default": "Ctrl + Q",
        "mac": "Command + E"
      },
      "description": "Toggle Command"
    }
  }
}
